<idea-plugin>
    <id>axan18.MethodCallPathDetectorPlugin</id>
    <name>MethodCallPathDetectorPlugin</name>
    <vendor email="bartosz_janusz731@protonmail.com" url="https://github.com/Axan18">Bartosz Janusz</vendor>

    <actions>
        <action id="axan18.methodcallpathdetectorplugin.MethodCallPathDetector"
                class="axan18.methodcallpathdetectorplugin.MethodCallPathDetector"
                text="Find Call Path to Method"
                description="Find call path to given method inside this function.">
            <add-to-group group-id="EditorPopupMenu" anchor="first"/>
        </action>
    </actions>
    <extensions defaultExtensionNs="com.intellij">
    </extensions>
</idea-plugin>
